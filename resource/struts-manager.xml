<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="manager" extends="struts-default" namespace="/manager">

		<!-- 用户 -->
		<action name="*Ur" method="{1}" class="appUserInfoAction"></action>
		
		<!-- 系统消息 -->
		<action name="*Msg" method="{1}" class="systemMessagerAction"></action>
		
		<!-- 企业任务 -->
		<action name="*Tsk" method="{1}" class="enterpriseTaskAction"></action>	
		
		<!-- 课程信息 -->
		<action name="*Cus" method="{1}" class="coursesAction"></action>	
		
	</package>
</struts>
